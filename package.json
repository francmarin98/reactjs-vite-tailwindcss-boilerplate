{
	"name": "vite-modyo-react-ts",
	"description": "This is a React + Vite boilerplate to be used with Tailwindcss.",
	"version": "0.0.1",
	"private": true,
	"author": {
		"name": "Francisco Marin",
		"url": "https://github.com/francmarin98"
	},
	"contributors": [
		{
			"name": "Jipson Saad",
			"url": "https://github.com/Exodiel"
		}
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"serve": "vite preview",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"coverage": "vitest run --coverage",
		"format": "prettier -uw --ignore-path .prettierignore .",
		"lint": "eslint src --max-warnings=0 --ignore-path .eslintignore",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"modyo-push": "modyo-cli push --publish"
	},
	"dependencies": {
		"@modyo/sdk": "^1.0.6",
		"@reduxjs/toolkit": "1.8.1",
		"axios": "0.26.1",
		"framer-motion": "^6",
		"i18next": "21.6.14",
		"liquidjs": "^9.36.0",
		"react-dom": "18.0.0",
		"react-i18next": "11.16.2",
		"react": "18.0.0"
	},
	"devDependencies": {
		"@modyo/cli": "^3.1.25",
		"@storybook/addon-actions": "6.4.19",
		"@storybook/addon-essentials": "6.4.19",
		"@storybook/addon-interactions": "6.4.19",
		"@storybook/addon-links": "6.4.19",
		"@storybook/react": "6.4.19",
		"@storybook/testing-library": "0.0.9",
		"@testing-library/jest-dom": "5.16.3",
		"@testing-library/react": "13.0.0",
		"@testing-library/user-event": "14.0.3",
		"@types/jest": "27.4.1",
		"@types/node": "17.0.23",
		"@types/react-dom": "17.0.14",
		"@types/react": "17.0.43",
		"@typescript-eslint/eslint-plugin": "5.17.0",
		"@typescript-eslint/parser": "5.17.0",
		"@vitejs/plugin-react": "1.3.0",
		"autoprefixer": "^10.4.4",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-prettier": "4.0.0",
		"eslint-plugin-react-hooks": "4.4.0",
		"eslint-plugin-react": "7.29.4",
		"eslint": "8.12.0",
		"jest": "27.5.1",
		"postcss": "^8.4.12",
		"prettier": "2.6.1",
		"storybook-builder-vite": "0.1.22",
		"tailwindcss": "^3.0.23",
		"typescript": "4.6.3",
		"vite-tsconfig-paths": "3.4.1",
		"vite": "2.9.1",
		"vitest": "0.8.1"
	}
}
